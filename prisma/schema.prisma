generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Student {
  id                  Int    @id @default(autoincrement())
  name                String
  age                 Int
  firstSemesterScore  Float
  secondSemesterScore Float
  teacherName         String
  roomNumber          Int
}
